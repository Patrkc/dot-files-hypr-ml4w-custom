{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"palette": {
		"os": "#ACB0BE",
		"closer": "p:os",
		"pink": "#F5C2E7",
		"lavender": "#B4BEFE",
		"blue": "#89B4FA",
		"yellow": "#F9E2AF",
		"peach": "#FAB387",
		"sapphire": "#74C7EC",
		"surface1": "#45475A"
	},
	"blocks": [
		{
			"alignment": "left",
			"segments": [
				{
					"foreground": "p:os",
					"style": "plain",
					"template": "{{.Icon}} ",
					"type": "os"
				},
				{
					"foreground": "p:blue",
					"style": "plain",
					"template": "{{ .UserName }}@{{ .HostName }} ",
					"type": "session"
				},
				{
					"foreground": "p:pink",
					"properties": {
						"folder_icon": "Óóæ",
						"home_icon": "~",
						"style": "agnoster_short"
					},
					"style": "plain",
					"template": "{{ .Path }} ",
					"type": "path"
				},
				{
					"type": "php",
					"style": "plain",
					"foreground": "p:sapphire",
					"template": " ÓúΩ {{ .Full }}",
					"foreground_templates": [
						"{{ if .Error }}p:red{{ else }}p:blue{{ end }}"
					]
				},
				{
					"type": "java",
					"style": "plain",
					"foreground": "p:lavender",
					"template": " Óú∏ {{ .Full }}",
					"foreground_templates": [
						"{{ if .Error }}p:red{{ else }}p:blue{{ end }}"
					]
				},
				{
					"type": "python",
					"style": "plain",
					"foreground": "p:green",
					"template": " Óàµ {{ .Full }}",
					"foreground_templates": [
						"{{ if .Error }}p:red{{ else }}p:yellow{{ end }}"
					]
				},
				{
					"type": "pnpm",
					"style": "plain",
					"foreground": "p:sky",
					"properties": {
						"display_mode": "files",
						"extensions": ["pnpm-lock.yaml"]
					},
					"template": "{{ .Full }}",
					"foreground_templates": [
						"{{ if .Error }}p:red{{ else }}p:peach{{ end }}"
					]
				},
				{
					"type": "npm",
					"style": "plain",
					"foreground": "p:teal",
					"properties": {
						"display_mode": "files",
						"extensions": ["package-lock.json"]
					},
					"template": "{{ .Full }}",
					"foreground_templates": [
						"{{ if .Error }}p:red{{ else }}p:yellow{{ end }}"
					]
				},
				{
					"template": "Óúò ÓµÑ  {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
					"foreground": "#50fa7b",
					"type": "node",
					"style": "plain"
				},
				{
					"type": "go",
					"background": "transparent",
					"style": "plain",
					"foreground": "#7FD5EA",
					"template": "ÔÑÖ Óòß Û∞üì  {{ .Full }} "
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "right",
			"segments": [
				{
					"type": "executiontime",
					"style": "plain",
					"foreground": "p:lavender",
					"template": " ÓÆ¢ {{ .FormattedMs }} ",
					"foreground_templates": [
						"{{ if gt .FormattedMs 500 }}p:peach{{ else }}p:blue{{ end }}"
					],
					"properties": {
						"threshold": 500,
						"style": "austin",
						"always_enabled": true
					}
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"type": "git",
					"style": "plain",
					"foreground": "p:lavender",
					"template": "ÔÑÖ{{ .UpstreamIcon }}{{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} ÔÉá {{ .StashCount }}{{ end }}",
					"properties": {
						"fetch_status": true,
						"fetch_upstream_icon": true,
						"branch_icon": "  Óú• ",
						"cherry_pick_icon": "\ue29b ",
						"commit_icon": "\uf417 ",
						"merge_icon": "\ue727 ",
						"no_commits_icon": "\uf0c3 ",
						"rebase_icon": "\ue728 ",
						"revert_icon": "\uf0e2 ",
						"tag_icon": "\uf412 ",
						"source": "cli",
						"mapped_branches": {
							"feat/*": "üöÄ ",
							"bug/*": "üêõ "
						}
					},
					"foreground_templates": [
						"{{ if or (.Working.Changed) (.Staging.Changed) }}p:yellow{{ end }}",
						"{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:peach{{ end }}",
						"{{ if gt .Ahead 0 }}p:blue{{ end }}",
						"{{ if gt .Behind 0 }}p:sapphire{{ end }}"
					]
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"newline": true,
			"segments": [
				{
					"style": "plain",
					"foreground": "p:closer",
					"template": " ÔÑÖ ",
					"type": "text"
				}
			],
			"type": "prompt"
		}
	],
	"transient_prompt": {
		"background": "transparent",
		"foreground": "p:closer",
		"template": " ÔÑÖ "
	},
	"final_space": true,
	"version": 3
}
